// PROGRAM TO PRINT TOTAL SUM OF ALL DIGITS  OF GIVEN NUMBER , 
//IF THE RESULT IS MORE THAN 2 DIGITS THEN REPEAT THE PROCESS

import java.util.Scanner;
class D
{
	public static void main(String[] args) 
	{
		Scanner scn = new Scanner(System.in);
		
		System.out.print("Enter a number : ");
		int num = scn.nextInt();
		
		int sum = 0;
		
		while(num>0){
			sum += num%10;
			num /= 10;
			
			if(num==0 && sum>9){
				num = sum;
				sum = 0;
			}
			else break;
		}
			
		System.out.println(sum);
			
		main(null);
	}
}


