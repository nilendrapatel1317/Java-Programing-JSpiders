package com.string;

public class Test {

	public static void main(String[] args) {
		System.out.println("* * * * * * *");
		System.out.println("* * * A * * *");
		System.out.println("* * A A A * *");
		System.out.println("* A A A A A *");
		System.out.println("* * A A A * *");
		System.out.println("* * * A * * *");
		System.out.println("* * * * * * *");

	}

}
